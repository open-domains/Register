{
    "description": "Project Description",
    "domain": "pomotime-me.is-cool.dev",
    "subdomain": "pomotime-me",

    "owner": {
        "repo": "https://github.com/poloni957/my-open-domains",
        "email": "gcampospoloni@outlook.com"
    },

    "record": {
        "A": ["127.0.0.1"],
        "AAAA": [":5500"],
        "CNAME": "pomotime-me.is-cool.dev",
        "MX": ["mx1.example.com", "mx2.example.com"],
        "TXT": ["example_verification=1234567890"],
